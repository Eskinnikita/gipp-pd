            </router-link>\n            </div>\n        </template>\n        <template v-else>\n            А новостей-то и нет вовсе\n        </template>\n    </div>\n</template>\n\n<script>\n    import {mapState} from 'vuex';\n\n    export default {\n        created() {\n            console.log(this.$route)\n            if(this.$route.path === '/') {\n                this.$store.commit('GET_ALL_NEWS')\n            }\n            else {\n                const rubric = this.pageModule.publisher.rubrics.find(el => el.uri === this.$route.params.name)\n                this.$store.commit('GET_RUBRIC_NEWS', rubric.id)\n            }\n        },\n        data() {\n            return {}\n        },\n        computed: {\n            ...mapState(['newsModule', 'pageModule'])\n        },\n    }\n</script>\n\n<style scoped>\n    .news-block {\n        font-size: 30px;\n        font-weight: 600;\n        margin-bottom: 30px;\n    }\n</style>"],"sourceRoot":"src/views"}]}